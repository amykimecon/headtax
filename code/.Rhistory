group_by(group) %>% summarize(across(-c(WEIGHT), .fns = c(~round(weighted.mean(.x, WEIGHT, na.rm=TRUE), 2), #weighted mean
~ifelse(is.na(mean(.x,na.rm=TRUE)),
NA,
paste0("(", round(sqrt(wtd.var(.x, WEIGHT, na.rm=TRUE)), 2), ")")))), #weighted sd
OBS = n())
